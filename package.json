{
  "name": "random-word-slugs",
  "version": "0.1.6",
  "description": "A random word slug generator pre-loaded with many words",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest --coverage",
    "prepublish": "tsc",
    "populate-readme": "npx ts-node ./scripts/populateReadme.ts"
  },
  "keywords": [
    "random words",
    "random slugs",
    "slug",
    "slug generator",
    "javascript",
    "typescript"
  ],
  "author": "Nick Scialli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nas5w/random-word-slugs/issues"
  },
  "homepage": "https://github.com/nas5w/random-word-slugs#readme",
  "lint-staged": {
    "*": "npm run populate-readme"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "husky": "^7.0.4",
    "jest": "^27.2.4",
    "lint-staged": "^11.2.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.1.3"
  }
}
